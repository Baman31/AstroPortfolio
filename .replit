modules = ["php-8.2", "web", "nodejs-20"]
[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "webserver"

[[workflows.workflow]]
name = "webserver"
author = "agent"

[[workflows.workflow.tasks]]
